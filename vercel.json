{
  "env": {
    "VITE_API_URL": "https://teamtrack-backend-wwo6.onrender.com/api"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://teamtrack-backend-wwo6.onrender.com/api"
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
